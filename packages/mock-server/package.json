{
  "name": "@sigstore/mock-server",
  "version": "0.3.1",
  "private": "true",
  "description": "Sigstore Mock Server",
  "bin": {
    "server": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "clean": "shx rm -rf dist",
    "prebuild": "npm run clean",
    "build": "tsc -b",
    "start": "./bin/run"
  },
  "dependencies": {
    "@oclif/color": "^1.0.13",
    "@oclif/core": "^4",
    "@sigstore/mock": "^0.12.0",
    "@tufjs/repo-mock": "^4.0.1",
    "express": "5.2.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "oclif": "^4",
    "tslib": "^2.8.1"
  },
  "oclif": {
    "bin": "server",
    "commands": {
      "strategy": "single",
      "target": "./dist/index.js"
    }
  },
  "engines": {
    "node": "^20.17.0 || >=22.9.0"
  }
}
